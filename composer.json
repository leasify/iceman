{
    "name": "leasify/iceman",
    "description": "Internal pipeline tool at Leasify",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://leasify.se",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Ek",
            "email": "andreas.ek@leasify.se"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "mockery/mockery": "*",
        "pestphp/pest": "*",
        "laravel-zero/framework": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/iceman"]
}
